		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@unocss/extractor-svelte": "^0.53.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.3.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^4.14.1",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"unocss": "^0.53.0",
		"vite": "^4.2.0",
		"vitest": "^0.32.0"
